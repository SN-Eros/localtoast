      "-OR-IF- the xinetd package is required as a dependency:\n"
      "```\n"
      "# systemctl stop xinetd.service"
      "# systemctl mask xinetd.service"
      "```\n"
    cis_benchmark: {
      profile_level: 1
      severity: MEDIUM
    }
