      severity: LOW
    }
    scan_instructions:
      "generic:{"
      "  check_alternatives:{"
      "    file_checks:{"
      "    files_to_check:{single_file:{path:\"/usr/lib/systemd/systemd-journal-remote\"}}"
      "    existence:{should_exist:true}"
      "    }"
      "}}"
  }
}
