      "      content_entry:{"
      "        match_type: NONE_MATCH"
      "        match_criteria:{"
      "          filter_regex: \"[^#]*\\\\s*ForwardToSyslog\\\\s*=\\\\s*yes.*\""
      "          expected_regex: \"[^#]*\\\\s*ForwardToSyslog\\\\s*=\\\\s*yes.*\""
      "        }"
