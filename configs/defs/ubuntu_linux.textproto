      "      recursive: false"
      "    }}"
      "    content_entry:{"
      "      match_type: ALL_MATCH_ANY_ORDER"
      "      match_criteria: {"
      "        filter_regex: \"[^#]*\\\\bpam_pwquality.so.*\""
      "        expected_regex: \"[^#]*\\\\bpam_pwquality.so.*\""
      "      }"
      "    }"
      "  }"
      "}}"
  }
