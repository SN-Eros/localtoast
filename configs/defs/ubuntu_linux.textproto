      "        match_type: ALL_MATCH_STRICT_ORDER"
      "        match_criteria:{"
      "          filter_regex: \"\\\\s*-A INPUT -i lo -j ACCEPT\""
      "          expected_regex: \"\\\\s*-A INPUT -i lo -j ACCEPT\""
      "        }"
      "        match_criteria:{"
      "          filter_regex: \"\\\\s*-A INPUT -s 127\\\\.0\\\\.0\\\\.0/8 -j DROP\""
