      "      content_entry:{"
      "        match_type: ALL_MATCH_ANY_ORDER"
      "        match_criteria: {"
      "          filter_regex:  \"[^#]*\\\\bspace_left_action\\\\s*=\\\\s*\\\\b(email|exec|single|halt)\\\\b.*\""
      "          expected_regex: \"[^#]*\\\\bspace_left_action\\\\s*=\\\\s*\\\\b(email|exec|single|halt)\\\\b.*\""
