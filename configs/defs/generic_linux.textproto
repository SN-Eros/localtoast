      "        recursive: true"
      "      }}"
      "      content_entry: {"
      "        match_type: ALL_MATCH_ANY_ORDER"
      "        match_criteria: {"
      "          filter_regex: \"net.ipv4.ip_forward *=.*\""
      "          expected_regex: \"net.ipv4.ip_forward *= *0\""
      "        }"
      "        match_criteria: {"
      "          filter_regex: \"net.ipv6.conf.all.forwarding *=.*\""
