benchmark_configs: {
  id: "groups-from-etc-passwd-in-etc-group"
  compliance_note: {
    version: { cpe_uri: "cpe:/o:centos:centos:7.%02" version: "3.1.2" benchmark_document: "CIS Centos Linux 7.x" }
    version: { cpe_uri: "cpe:/o:cos:cos_linux:93" version: "1.0.0" benchmark_document: "CIS Container-Optimized OS" }
    version: { cpe_uri: "cpe:/o:cos:cos_linux:97" version: "1.0.0" benchmark_document: "CIS Container-Optimized OS" }
    version: { cpe_uri: "cpe:/o:cos:cos_linux:101" version: "1.0.0" benchmark_document: "CIS Container-Optimized OS" }
    version: { cpe_uri: "cpe:/o:cos:cos_linux:105" version: "1.0.0" benchmark_document: "CIS Container-Optimized OS" }
    version: { cpe_uri: "cpe:/o:cos:cos_linux:109" version: "1.0.0" benchmark_document: "CIS Container-Optimized OS" }
    version: { cpe_uri: "cpe:/o:cos:cos_linux:113" version: "1.0.0" benchmark_document: "CIS Container-Optimized OS" }
    version: { cpe_uri: "cpe:/o:cos:cos_linux:117" version: "1.0.0" benchmark_document: "CIS Container-Optimized OS" }
    version: { cpe_uri: "cpe:/o:rockylinux:rockylinux:8.5" version: "1.0.0" benchmark_document: "CIS Rocky Linux 8" }
    title: "Ensure all groups in /etc/passwd exist in /etc/group"
    description:
      "Over time, system administration errors and changes can lead to groups "
      "being defined in /etc/passwd but not in /etc/group."
    rationale:
      "Groups defined in the /etc/passwd file but not in the /etc/group file "
      "pose a threat to system security since group permissions are not "
      "properly managed."
    remediation:
      "Make sure that all groups defined in the /etc/passwd file are also "
      "present in the /etc/group file."
    cis_benchmark: {
      profile_level: 1
      severity: LOW
    }
    scan_instructions:
      "generic:{check_alternatives:{"
      "  file_checks:{"
      "    files_to_check:{single_file:{path:\"/etc/group\"}}"
      "    content_entry:{"
      "      match_type: ALL_MATCH_ANY_ORDER"
      "      match_criteria: {"
      "        filter_regex: \".*:.*:$gid:.*\""
      "        expected_regex: \".*:.*:$gid:.*\""
      "      }"
      "    }"
      "    repeat_config:{type: FOR_EACH_USER_WITH_LOGIN}"
      "    non_compliance_msg: \"a GID from /etc/passwd isn't present in /etc/group\""
      "    file_display_command:"
      "      \"#!/bin/bash\\n\""
      "      \"for i in $(cut -s -d: -f4 /etc/passwd | sort -u ); do\\n\""
      "      \" grep -q -P \\\"^.*?:[^:]*:$i:\\\" /etc/group\\n\""
      "      \" if [ $? -ne 0 ]; then\\n\""
      "      \"   echo \\\"Group $i is referenced by /etc/passwd but does not exist in /etc/group\\\"\\n\""
      "      \" fi\\n\""
      "      \"done\""
      "  }"
      "}}"
  }
}
