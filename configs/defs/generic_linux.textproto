      "      files_to_check:{files_in_dir:{"
      "        dir_path:\"/etc/sysctl.d/\""
      "        recursive: true"
      "      }}"
      "      content_entry: {"
      "        match_type: ALL_MATCH_ANY_ORDER"
      "        match_criteria: {"
      "          filter_regex: \"net.ipv4.tcp_syncookies *=.*\""
      "          expected_regex: \"net.ipv4.tcp_syncookies *= *1\""
      "        }"
